Algoritmo "Exercício 03"

Var
n1,n2,res: inteiro

Inicio
escreval("Algoritmo - Divisão de números")
escreval
//ENTRADA
escreva("Digite o primeiro número: ")
leia(n1)
escreval
escreva("Digite o segundo número : ")
leia(n2)
escreval

//PROCESSAMENTO
res <- n1\n2


//SAÍDA
escreva(n1," / ",n2," = ",res)


Fimalgoritmo